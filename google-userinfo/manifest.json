{

    "name": "Google User Info",
    "version": "1",
    "manifest_version": 2,
    "applications": {
      "gecko": {
        "id": "{a9a112f5-2a83-443e-b3db-a42ff9175f32}",
        "strict_min_version": "53a1"
      }
    },
  
    "icons": {
      "48": "icons/person-48.png"
    },
  
    "browser_action": {
      "browser_style": true,
      "default_icon": "icons/person-32.png",
      "default_title": "Drive",
      "default_popup": "popup/drive.html"
    },
  
    "permissions": [
      "tabs",
      "identity",
      "notifications",
      "*://www.googleapis.com/*",
      "*://accounts.google.com/*"
    ],
  
    "background": {
      "page": "background/back.html"
    },
  
    "options_ui": {
      "page": "options/options.html"
    }
  
  }